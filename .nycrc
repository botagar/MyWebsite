{
  "check-coverage": true,
  "per-file": true,
  "cache": true,
  "all": true,
  "lines": 0,
  "statements": 0,
  "functions": 0,
  "branches": 0,
  "exclude": [
    "**/*.spec.js",
    "dist",
    "src/client/main.js",
    "src/client/redux",
    "src/client/common/styles/*.js",
    "**/*.config.js",
    "src/client/common/initialState.js"
  ],
  "include": [
    "src/client/**/*.js",
    "src/client/**/*.jsx"
  ],
  "reporter": [
    "lcov",
    "html",
    "text",
    "text-summary"
  ]
}